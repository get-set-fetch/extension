{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 Chrome versions", "last 2 Firefox versions"]
      },
      "useBuiltIns": true,
      "debug": false
    }],
    "react"
  ]
  ,
  "plugins": [
    "transform-class-properties",
    ["module-resolver", {
      "root": ["./"]
    }]
  ]
  
}