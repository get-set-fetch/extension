    "name": "Andrei Sabau"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/get-set-fetch/extension/issues"
  },
  "homepage": "https://github.com/get-set-fetch/extension#readme",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "get-set-fetch": "^0.3.8",
    "get-set-fetch-extension-commons": "file:../commons",
    "jszip": "^3.2.0",
    "pako": "^1.0.10",
    "webextension-polyfill": "^0.6.0",
    "untar.js": "^0.3.0",
    "url-parse": "^1.4.3"
  }
