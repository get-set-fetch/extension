{
  "name": "get-set-fetch-extension-background",
  "version": "1.0.0",
  "description": "",
  "main": "dist/background-bundle.js",
  "module": "dist/background-bundle.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:dist && npm run copy:resources",
    "build:dist": "rollup --config rollup.config.js",
    "copy:resources": "npm run copy:resources:html && npm run copy:resources:systemjs",
    "copy:resources:html": "copyfiles -u 2 src/html/*.html dist",
    "copy:resources:systemjs": "copyfiles -f node_modules/systemjs/dist/system.js dist/plugins/systemjs && copyfiles -f node_modules/systemjs-plugin-babel/plugin-babel.js node_modules/systemjs-plugin-babel/systemjs-babel-browser.js dist/plugins/systemjs",
    "test": "mocha test",
    "test:single": "mocha test --grep \"Test Site Crawl\""
  },
  "keywords": [
    "scraper",
    "extension"
  ],
  "author": "Andrei Sabau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/get-set-fetch/extension/issues"
  },
  "homepage": "https://github.com/get-set-fetch/extension#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "indexeddbshim": "^3.8.0",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.4",
    "puppeteer": "^1.11.0",
    "rimraf": "^2.6.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sinon": "^6.1.4"
  },
  "dependencies": {
    "get-set-fetch": "^0.3.8",
    "patch-package": "^5.1.1",
    "systemjs": "^0.21.4",
    "systemjs-plugin-babel": "0.0.25",
    "url-parse": "^1.4.3"
  }
}
