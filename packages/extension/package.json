{
  "name": "get-set-fetch-extension",
  "version": "1.0.0",
  "description": "",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run copy:resources",
    "copy:resources": "npm run copy:local && npm run copy:bkg && npm run copy:popup && npm run copy:admin && npm run copy:scenarios",
    "copy:local": "cpx \"src/resources/**/*\" dist",
    "copy:bkg": "cpx  \"node_modules/get-set-fetch-extension-background/dist/**/*\" dist/background",
    "copy:popup": "cpx \"node_modules/get-set-fetch-extension-popup/dist/**/*\" dist/popup",
    "copy:admin": "cpx \"node_modules/get-set-fetch-extension-admin/dist/**/*\" dist/admin",
    "copy:scenario-extract-resource": "cpx \"node_modules/get-set-fetch-scenario-extract-resources/dist/**/*\" dist/scenarios",
    "copy:scenarios": "npm run copy:scenario-extract-resource",
    "build:watch:admin": "npm run build && npm run copy:admin:watch",
    "build:watch:popup": "npm run build && npm run copy:popup:watch",
    "build:watch:bkg": "npm run build && npm run copy:bkg:watch",
    "copy:local:watch": "cpx \"src/resources/**/*\" dist --watch",
    "copy:bkg:watch": "cpx  \"node_modules/get-set-fetch-extension-background/dist/**/*\" dist/background --watch",
    "copy:popup:watch": "cpx \"node_modules/get-set-fetch-extension-popup/dist/**/*\" dist/popup --watch",
    "copy:admin:watch": "cpx \"node_modules/get-set-fetch-extension-admin/dist/**/*\" dist/admin --watch",
    "test": "mocha test \"./test/integration/**/*.ts\" --exit",
    "test:single": "mocha test \"./test/integration/**/*.ts\" --grep \"Scenario Builtin List\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "get-set-fetch": "^0.3.8",
    "get-set-fetch-extension-background": "1.0.0",
    "get-set-fetch-extension-popup": "^1.0.0",
    "get-set-fetch-extension-admin": "^1.0.0",
    "get-set-fetch-scenario-extract-resources": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/connect": "^3.4.32",
    "@types/mocha": "^5.2.5",
    "@types/puppeteer": "^1.12.1",
    "@types/serve-static": "^1.13.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "connect": "^3.6.6",
    "cpx": "^1.5.0",
    "jszip": "^3.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.4",
    "puppeteer": "^1.6.1",
    "query-string": "^6.1.0",
    "serve-static": "^1.13.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3",
    "vhost": "^3.0.2"
  }
}
