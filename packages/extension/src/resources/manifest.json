{

  "manifest_version": 2,
  "name": "Get Set Fetch",
  "version": "{GSF_VERSION}",

  "description": "Get Set Fetch",

  "icons": {
    "32": "icons/gsf_32x32.png"
  },

  "background": {
    "page": "background/background.html"
  },

  "browser_action": {
      "default_icon": "icons/gsf_32x32.png",
      "default_title": "Get Set Fetch"
    },

  "web_accessible_resources": [
    "admin/*"
  ],

  "permissions": ["tabs", "activeTab", "webRequest", "<all_urls>"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' blob: ; object-src 'self' blob: "
}